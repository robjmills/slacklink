{
   "name": "Slacklink",
   "description": "Send currently open page URL to slack channel",
   "version": "0.0.17",
   "background": {
	  "scripts": [
		 "assets/js/jquery-2.1.0.min.js"
	  ]
   },
   "permissions": [
	  "tabs"
   ],
   "options_page": "options.html",
   "web_accessible_resources": ["options.html"],
   "browser_action": {
	  "default_icon": "assets/images/slacklink_128.png",
	  "default_title": "Slack Link",
	  "default_popup": "popup.html"
   },
   "icons":{
	  "16": "assets/images/slacklink_16.png",
	  "48": "assets/images/slacklink_48.png",
	  "128": "assets/images/slacklink_128.png"
   },

   "manifest_version": 2,
   "update_url": "https://github.com/robjmills/slacklink/raw/master/update-manifest.xml"
}
